{
  "name": "@cannislabs/react-lib",
  "version": "0.0.3",
  "description": "React library for Cannis Labs",
  "main": "lib/public-api.js",
  "types": "lib/public-api.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sass": "^1.51.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitordhers/cannis-labs-react-lib.git"
  },
  "keywords": [
    "cannislabs",
    "react"
  ],
  "author": "Vitor Dhers",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vitordhers/cannis-labs-react-lib/issues"
  },
  "homepage": "https://github.com/vitordhers/cannis-labs-react-lib#readme",
  "files": [
    "lib/**/*"
  ]
}